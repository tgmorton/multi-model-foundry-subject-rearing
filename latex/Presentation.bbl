      \field{eprinttype}{arXiv}
      \field{journaltitle}{arXiv [q-bio.NC]}
      \field{month}{4}
      \field{title}{Explanatory models in neuroscience: Part 1 -- taking mechanistic abstraction seriously}
      \field{year}{2021}
    \endentry
